{
  "name": "vehicleModel",
  "base": "PersistedModel",
  "strict": false,
  "idInjection": false,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "vehicles": {
      "type": "hasMany",
      "model": "vehicle",
      "foreignKey": "vehicleModelId"
    },
    "vehicleType": {
      "type": "belongsTo",
      "model": "vehicleType",
      "foreignKey": "vehicleTypeId"
    },
    "vehicleBrand": {
      "type": "belongsTo",
      "model": "vehicleBrand",
      "foreignKey": "vehicleBrandId"
    },
    "vehicleEngines": {
      "type": "hasAndBelongsToMany",
      "model": "vehicleEngine",
      "foreignKey": "vehicleEngineId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
