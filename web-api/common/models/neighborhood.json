{
  "name": "neighborhood",
  "base": "PersistedModel",
  "strict": false,
  "idInjection": false,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "cityId": {
      "type": "string"
    },
    "stateId": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "addresses": {
      "type": "hasMany",
      "model": "address",
      "foreignKey": "neighborhoodId"
    },
    "city": {
      "type": "belongsTo",
      "model": "city",
      "foreignKey": "cityId"
    },
    "state": {
      "type": "belongsTo",
      "model": "state",
      "foreignKey": "stateId"
    },
    "gasStations": {
      "type": "hasMany",
      "model": "gasStation",
      "foreignKey": "neighborhoodId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
