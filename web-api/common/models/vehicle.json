{
	"name": "vehicle",
	"base": "PersistedModel",
	"strict": false,
	"idInjection": false,
	"options": {
		"validateUpsert": true
	},
	"properties": {
		"nickName": {
			"type": "string",
			"required": true
		},
		"photo": {
			"type": "object",
			"photo": {
				"type": "string"
			},
			"thumb": {
				"type": "string"
			}
		},
		"year": {
			"type": "number"
		},
		"consumption": {
			"type": "number",
			"required": true,
			"comments": "km/l"
		},
		"power": {
			"type": "number",
			"comments": "potencia do motor (ex.: 1.1 16v)"
		},
		"vehicleTypeId": {
			"type": "string",
			"required": true
		},
		"vehicleModelId": {
			"type": "string",
			"required": true
		},
		"vehicleBrandId": {
			"type": "string",
			"required": true
		},
		"ownerId": {
			"type": "string",
			"required": true
		}
	},
	"validations": [],
	"relations": {
		"vehicleType": {
			"type": "belongsTo",
			"model": "vehicleType",
			"foreignKey": "vehicleTypeId"
		},
		"user": {
			"type": "belongsTo",
			"model": "user",
			"foreignKey": "ownerId"
		},
		"vehicleBrand": {
			"type": "belongsTo",
			"model": "vehicleBrand",
			"foreignKey": "vehicleBrandId"
		},
		"vehicleModel": {
			"type": "belongsTo",
			"model": "vehicleModel",
			"foreignKey": "vehicleModelId"
		}
	},
	"acls": [{
		"accessType": "*",
		"principalType": "ROLE",
		"principalId": "$unauthenticated",
		"permission": "DENY"
	}, {
		"accessType": "*",
		"principalType": "ROLE",
		"principalId": "$everyone",
		"permission": "DENY"
	}, {
		"accessType": "*",
		"principalType": "ROLE",
		"principalId": "$owner",
		"permission": "ALLOW"
	}],
	"methods": {}
}
