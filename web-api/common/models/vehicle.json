{
  "name": "vehicle",
  "base": "PersistedModel",
  "strict": false,
  "idInjection": false,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "year": {
      "type": "number"
    },
    "consumption": {
      "type": "number",
      "required": true,
      "comments": "km/l"
    },
    "power": {
      "type": "number",
      "comments": "potencia do motor (ex.: 1.1 16v)"
    },
    "model": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "vehicleType": {
      "type": "belongsTo",
      "model": "vehicleType",
      "foreignKey": "vehicleTypeId"
    },
    "user": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "ownerId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
