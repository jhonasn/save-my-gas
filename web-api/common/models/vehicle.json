{
  "name": "vehicle",
  "base": "PersistedModel",
  "strict": false,
  "idInjection": false,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "nickName": {
      "type": "string"
    },
    "year": {
      "type": "number"
    },
    "consumption": {
      "type": "number",
      "required": true,
      "comments": "km/l"
    },
    "vehicleTypeId": {
      "type": "string",
      "required": true
    },
    "vehicleModelId": {
      "type": "string",
      "required": true
    },
    "vehicleEngineId": {
      "type": "string",
      "required": true
    },
    "vehicleBrandId": {
      "type": "string",
      "required": true
    },
    "fuelTypeId": {
      "type": "string",
      "required": true
    },
    "ownerId": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "vehicleType": {
      "type": "belongsTo",
      "model": "vehicleType",
      "foreignKey": "vehicleTypeId"
    },
    "user": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "ownerId"
    },
    "vehicleBrand": {
      "type": "belongsTo",
      "model": "vehicleBrand",
      "foreignKey": "vehicleBrandId"
    },
    "vehicleModel": {
      "type": "belongsTo",
      "model": "vehicleModel",
      "foreignKey": "vehicleModelId"
    },
    "vehicleEngine": {
      "type": "belongsTo",
      "model": "vehicleEngine",
      "foreignKey": "vehicleEngineId"
    },
    "fuelType": {
      "type": "belongsTo",
      "model": "fuelType",
      "foreignKey": "fuelTypeId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
