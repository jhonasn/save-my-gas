<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<widget id="com.ionicframework.savemygas491016" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>save-my-gas</name>
    <description>
        An Ionic Framework and Cordova project.
    </description>
    <author email="jhonasn@gmail.com" href="http://ionicframework.com/">
        Jhonas Nascimento
    </author>
    <content src="index.html" />

    <access origin="*" />
    <allow-navigation href="*" />

    <preference name="webviewbounce" value="false" />
    <preference name="UIWebViewBounce" value="false" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="BackupWebStorage" value="none" />
    <feature name="StatusBar">
        <param name="ios-package" value="CDVStatusBar" onload="true" />
    </feature>
</widget>
